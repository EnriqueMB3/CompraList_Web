
@model IEnumerable<CompraList_Web.Models.Item>

<ul id="ul-items">
    @foreach (var item in Model)
    {
        <li>
            <div class="item-list">
                <div>
                    <input class="form-check-input " type="checkbox" checked="@item.Listo" onclick="ChangeStatus(@item.Id)">
                    <label class="form-check-label" for="defaultCheck1">@item.Nombre</label>
                </div>
                <span class="badge badge-pill badge-primary">@item.QuienAgrego</span>
                <i class="fas fa-trash-alt trash" onclick="DeleteItem(@item.Id)"></i>
            </div>
        </li>
    }
</ul>

